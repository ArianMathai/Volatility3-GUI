{
  "name": "v3-analyser",
  "productName": "V3Analyser",
  "author": "Libra dream team",
  "description": "A gui for volatility 3",
  "version": "1.0.0",
  "main": "frontend/main.js",
  "scripts": {
    "start": "cd frontend && npm run start",
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "package": "yarn electronBuild",
    "electronBuild": "electron-builder"
  },
  "dependencies": {
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "electron": "^30.0.9",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "asar": true,
    "asarUnpack": [
      "backend/**/*",
      "volatility3/**/*"
    ],
    "appId": "com.libra.V3Analyser",
    "files": [
    "**/*",
    "!**/src/**/*",
    "!**/*.map",
    "!backend/memDump/**/*"
    ],
    "directories": {
      "buildResources": "./frontend/build/",
      "output": "../"
    }
  }
}